{
  "name": "prettier-plugin-ini",
  "version": "1.1.0",
  "description": "prettier plugin for INI files",
  "type": "module",
  "main": "src/plugin.js",
  "scripts": {
    "lint": "eslint --cache .",
    "prepare": "node bin/languages.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kddnewton/prettier-plugin-ini.git"
  },
  "author": "Kevin Newton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kddnewton/prettier-plugin-ini/issues"
  },
  "homepage": "https://github.com/kddnewton/prettier-plugin-ini#readme",
  "dependencies": {
    "prettier": ">=3.0.0-alpha.3"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "jest": "^27.0.1",
    "linguist-languages": "^7.21.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "jest": {
    "testRegex": ".test.js$",
    "transform": {}
  },
  "prettier": {
    "plugins": [
      "."
    ],
    "trailingComma": "none"
  }
}
